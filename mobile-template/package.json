{
  "name": "mobile-app-webview-template",
  "version": "1.0.0",
  "description": "Capacitor-based mobile app template for website wrapping with Firebase and Appsflyer integration",
  "main": "index.js",
  "scripts": {
    "build": "npm run copy && capacitor sync",
    "copy": "mkdir -p www && cp -r src/* www/",
    "dev": "npm run build && capacitor run android",
    "dev:ios": "npm run build && capacitor run ios",
    "android": "capacitor run android",
    "ios": "capacitor run ios",
    "sync": "capacitor sync",
    "clean": "rimraf www dist node_modules/.capacitor",
    "prebuild": "rimraf www",
    "build:android": "npm run build && capacitor build android",
    "build:ios": "npm run build && capacitor build ios"
  },
  "dependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/browser": "^5.2.0",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/core": "^5.6.0",
    "@capacitor/device": "^5.0.6",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/geolocation": "^5.0.6",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/network": "^5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/toast": "^5.0.6",
    "firebase": "^10.7.1",
    "appsflyer-capacitor-plugin": "6.14.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "capacitor",
    "mobile-app",
    "webview",
    "firebase",
    "appsflyer",
    "ionic",
    "hybrid-app"
  ],
  "author": "YosShor",
  "license": "MIT",
  "capacitor": {
    "appId": "com.example.webview",
    "appName": "WebView App",
    "webDir": "www",
    "server": {
      "androidScheme": "https"
    }
  }
}
